module Effect.Empty : Module

import Data.Bool
import Data.Function
import Data.Option
import Data.Unit

interface Empty : Interface
{ empty : { A : Type } -> A }

guard : (c : Bool) -> [Empty] Unit
{ if c id { (unit) -> empty } }


toOption : {A : Type} -> [Empty] A -> Option A
{ [empty ; _] -> none
, a           -> some a }
